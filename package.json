{
  "version": "1.1.0",
  "keywords": [
    "strapi",
    "api.video",
    "strapi5",
    "strapi-cms",
    "cms",
    "headlesscms"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=20 <=24"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "@api.video/nodejs-client": "^2.6.8",
    "@api.video/private-video-session": "^0.1.0",
    "@api.video/react-player": "^1.1.0",
    "@api.video/video-uploader": "^1.1.6",
    "@strapi/design-system": "^2.1.2",
    "@strapi/icons": "^2.1.2",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-intl": "^8.1.1"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.34.0",
    "@strapi/typescript-utils": "^5.34.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "commit-and-tag-version": "^12.6.1",
    "prettier": "^3.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "api-video-strapi-5-plugin",
    "displayName": "api.video Strapi 5 Plugin",
    "description": "A powerful Strapi plugin to easily manage your videos and integrate them in your project"
  },
  "name": "@strawbinary-io/api-video-strapi-5-plugin",
  "description": "A powerful Strapi plugin to easily manage your videos and integrate them in your project",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Strawbinary/api.video-strapi-5-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/Strawbinary/api.video-strapi-5-plugin/issues"
  },
  "homepage": "https://strawbinary.de/",
  "author": "Strawbinary team <info@strawbinary.de>"
}
